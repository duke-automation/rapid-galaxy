[server:main]
host = {{ galaxy_listen_host }}
port = 8080
use = egg:Paste#http
use_threadpool = True
threadpool_kill_thread_limit = 10800

[filter:gzip]
use = egg:Paste#gzip

[filter:proxy-prefix]
use = egg:PasteDeploy#prefix
prefix = /galaxy

[app:main]
paste.app_factory = galaxy.web.buildapp:app_factory

use_nglims = False
nglims_config_file = tool-data/nglims.yaml

use_interactive = True
admin_users = rapiduser@duke.edu, liz@duke.edu, clc0317@duke.edu

[galaxy_amqp]
